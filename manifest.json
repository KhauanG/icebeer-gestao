  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["text/csv", ".csv", "application/vnd.ms-excel", ".xlsx"]
        }
      ]
    }
  },

  "protocol_handlers": [
    {
      "protocol": "web+icebeer",
      "url": "/?handler=%s"
    }
  ],

  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xlsx"],
        "application/json": [".json"]
      }
    }
  ],

  "edge_side_panel": {
    "preferred_width": 480
  },

  "launch_handler": {
    "client_mode": "focus-existing"
  },

  "handle_links": "preferred",

  "scope_extensions": [
    {"origin": "https://gestao-ice-beer.firebaseapp.com"},
    {"origin": "https://gestao-ice-beer.web.app"}
  ],

  "permissions": [
    "storage",
    "notifications",
    "background-sync",
    "background-processing"
  ],

  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://gestao-ice-beer.firebaseapp.com/manifest.json"
    }
  ],

  "prefer_related_applications": false,

  "id": "ice-beer-gestao-v4",
